{
  "$schema": "./zowe.schema.json",
  "profiles": {
    "zosmf": {
      "type": "zosmf",
      "properties": {
        "port": <PORT>
      },
      "secure": []
    },
    "tso": {
      "type": "tso",
      "properties": {
        "account": "",
        "codePage": "1047",
        "logonProcedure": "IZUFPROC"
      },
      "secure": []
    },
    "ssh": {
      "type": "ssh",
      "properties": {
        "port": 22
      },
      "secure": []
    },
    "base": {
      "type": "base",
      "properties": {
        "host": <HOST>,
        "rejectUnauthorized": <SSL>
      },
      "secure": [
        "user",
        "password"
      ]
    }
  },
  "defaults": {
    "zosmf": "zosmf",
    "tso": "tso",
    "ssh": "ssh",
    "base": "base"
  },
  "autoStore": true
}